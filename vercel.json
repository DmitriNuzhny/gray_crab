{
  "version": 2,
  "buildCommand": "yarn build",
  "outputDirectory": "dist",
  "framework": "node",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
} 